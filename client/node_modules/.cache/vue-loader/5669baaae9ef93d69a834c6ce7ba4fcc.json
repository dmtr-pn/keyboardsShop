e === \"ADMIN\") this.isAdmin = true\r\n        }, err => {\r\n            this.$store.dispatch('logout')\r\n            this.$notify({\r\n                title: \"Ошибка\",\r\n                text: err.response.data.error,\r\n                duration: 5000,\r\n                type: 'error',\r\n            });\r\n        })\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.profile__ {\r\n    display: flex;\r\n}\r\n.profile__menu {\r\n    min-width: 300px;\r\n}\r\n.profile__menu-item {\r\n    display: flex;\r\n    align-items: center;\r\n    padding: 10px;\r\n    font-size: 20px;\r\n}\r\n.profile__menu-item:hover {\r\n    background: #EEEEEE;\r\n    cursor: pointer;\r\n}\r\n.profile__menu-item-icn {\r\n    margin-right: 10px;\r\n}\r\n.profile__main {\r\n    width: 100%;\r\n    margin-left: 20px;\r\n}\r\n.down__mobile {\r\n    display: none;\r\n    margin-bottom: 20px;\r\n}\r\n.down__mobile-btn {\r\n    width: 100%;\r\n    font-weight: 700;\r\n    font-size: 24px;\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: center;\r\n    align-items: center;\r\n    padding: 10px 0px;\r\n    border: 2px solid #000000;\r\n    border-radius: 10px;\r\n    background-color: transparent;\r\n}\r\n\r\n@media screen and (max-width: 768px) { \r\n    .down__mobile {\r\n        display: block;\r\n    }\r\n}\r\n</style>"]}]}